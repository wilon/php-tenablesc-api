{
    "name": "wilon/php-tenablesc-api",
    "type": "library",
    "description": "Tenable SecurityCenter Api",
    "homepage": "https://github.com/wilon/php-tenablesc-api",
    "keywords": ["tenable", "SecurityCenter", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Weilong Wang",
            "homepage": "https://github.com/wilon"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "psr/http-message": "^1.0",
        "psr/cache": "^1.0",
        "php-http/httplug": "^1.1",
        "php-http/discovery": "^1.0",
        "php-http/client-common": "^1.3",
        "php-http/cache-plugin": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5 || ^6.0",
        "php-http/guzzle6-adapter": "^1.0",
        "php-http/mock-client": "^1.0",
        "guzzlehttp/psr7": "^1.2",
        "sllh/php-cs-fixer-styleci-bridge": "^1.3",
        "cache/array-adapter": "^0.4"
    },
    "autoload": {
        "psr-4": { "SecurityCenter\\": "src/SecurityCenter/" }
    },
    "autoload-dev": {
        "psr-4": { "SecurityCenter\\Tests\\": "test/SecurityCenter/Tests/"}
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
